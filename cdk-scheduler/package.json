{
  "name": "cdk-scheduler",
  "description": "Precise-to-the-second CDK construct to schedule events",
  "version": "0.1.7",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/guiyom-e/cdk-scheduler",
    "type": "git"
  },
  "keywords": [
    "cdk",
    "aws",
    "awscdk",
    "serverless",
    "lambda",
    "sqs",
    "dynamodb",
    "precise-to-the-second",
    "scheduling"
  ],
  "maintainers": [
    {
      "name": "Adèle Gauvrit"
    },
    {
      "name": "Guillaume Égée"
    }
  ],
  "main": "build/lib/index.js",
  "types": "build/lib/index.d.ts",
  "files": [
    "build/lib"
  ],
  "scripts": {
    "build": "tsc && yarn babel build/lib --out-dir build/lib --extensions \".js\"",
    "publish": "yarn build && npm publish --access public",
    "watch": "tsc -w",
    "test-type": "tsc --noEmit",
    "test-lint": "eslint . --ext js,ts --ignore-path ../.gitignore",
    "test-unit": "jest",
    "test": "yarn test-lint && yarn test-type && yarn test-unit"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@types/jest": "^26.0.10",
    "@types/lodash": "^4.14.180",
    "@types/node": "10.17.27",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "aws-cdk-lib": "^2.24.1",
    "babel-plugin-search-and-replace": "^1.1.0",
    "cdk": "^2.24.1",
    "constructs": "^10.0.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^26.4.2",
    "jest-esm-transformer": "^1.0.0",
    "prettier": "^2.6.1",
    "ts-jest": "^26.2.0",
    "typescript": "~3.9.7"
  },
  "packageManager": "yarn@3.1.1",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.58.0",
    "aws-sdk": "^2.1105.0",
    "lodash": "^4.17.21"
  },
  "engines": {
    "node": "14.18.2"
  }
}
